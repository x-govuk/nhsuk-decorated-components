{
  "name": "nhsuk-decorated-components",
  "version": "0.0.7",
  "description": "Form components for the NHS.UK Design System that require less parameters to collect data",
  "keywords": [
    "nhsuk",
    "design system",
    "prototyping",
    "components"
  ],
  "homepage": "https://github.com/x-govuk/nhsuk-decorated-components#readme",
  "bugs": {
    "url": "https://github.com/x-govuk/nhsuk-decorated-components/issues"
  },
  "license": "MIT",
  "files": [
    "lib",
    "x-nhsuk"
  ],
  "main": "lib/decorate.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/x-govuk/nhsuk-decorated-components.git"
  },
  "scripts": {
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "lint:js": "eslint '**/*.js'",
    "lint:js:fix": "eslint '**/*.js' --fix",
    "lint": "npm run lint:prettier && npm run lint:js",
    "lint:fix": "npm run lint:prettier:fix && npm run lint:js:fix",
    "test": "NODE_ENV=test node --test",
    "coverage": "NODE_ENV=test node --test --experimental-test-coverage",
    "release": "np"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.0.6",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "nhsuk-frontend": "^8.0.0",
    "np": "^9.2.0",
    "nunjucks": "^3.2.3"
  },
  "engines": {
    "node": ">=20"
  }
}
